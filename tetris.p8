--collision

--wall collision positive x
function wcpx (b,x,r)
	for px=0,3 do
		for py=0,3 do
			c = sget(bbx+rot(px,py,r),b)
			if (c != wc) then
				if (px+x>=ww) then
					return true
				end
			end
		end
	end
	return false
end

--wall collision negative x
function wcnx (b,x,r)
	for px=0,3 do
		for py=0,3 do
			c = sget(bbx+rot(px,py,r),b)
			if (c != wc) then
				if (px+x<0) then
					return true
				end
			end
		end
	end
	return false
end

--wall collision positive y
function wcpy (b,y,r)
	for px=0,3 do
		for py=0,3 do
			c = sget(bbx+rot(px,py,r),b)
			if (c != wc) then
				if (py+y>=wh) then
					return true
				end
			end
		end
	end
	return false
end

--check fits
function fits (b,x,y,r)
	for px=0,3 do
		for py=0,3 do
			c = sget(bbx+rot(px,py,r),b)
			if (c != wc) then
				i = ww*(flr(y)+py) + (flr(x)+px)
				if (buffer[i+1] != wc) then
					return false
				end 
			end
		end
	end
	return true
end
